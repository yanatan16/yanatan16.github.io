---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Jon Eisen's Blog</title>
 <link href="http://yanatan16.github.com/blog/atom.xml" rel="self"/>
 <link href="http://yanatan16.github.com/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://yanatan16.github.com/blog/</id>
 <author>
   <name>Jon Eisen</name>
   <email>jon.m.eisen@gmail.com</email>
 </author>
 
 {% for post in site.posts limit:5 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://yanatan16.github.com/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
